{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@hooks/*":["hooks/*"],
      "@models/*":["models/*"],
      "@components/*":["components/*"],
      "@utils/*":["utils/*"],
      "@store/*":["store/*"],
      "@pages/*":["pages/*"]
    }
  },
  "includes": [
    "src",
    "./external.d.ts"
  ]
}